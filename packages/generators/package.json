{
  "name": "@motionone/generators",
  "version": "10.5.2",
  "description": "A collection of animation generators.",
  "license": "MIT",
  "author": "Matt Perry",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf lib dist types && tsc -p . && rollup -c",
    "test": "jest --coverage --config jest.config.js",
    "dev": "rollup -c -w",
    "measure": "bundlesize"
  },
  "dependencies": {
    "@motionone/types": "^10.5.2",
    "@motionone/utils": "^10.5.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@motionone/easing": "^10.5.2"
  },
  "bundlesize": [
    {
      "path": "./dist/size-glide.js",
      "maxSize": "0.9 kB"
