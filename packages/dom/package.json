{
  "name": "@motionone/dom",
  "version": "10.5.2",
  "description": "A tiny, performant animation library for the DOM",
  "license": "MIT",
  "author": "Matt Perry",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf lib dist types && tsc -p . && webpack --config webpack.config.js && rollup -c",
    "test": "jest --coverage --config jest.config.js",
    "dev": "rollup -c -w",
    "measure": "bundlesize"
  },
  "dependencies": {
    "@motionone/easing": "^10.5.2",
    "@motionone/utils": "^10.5.2",
    "hey-listen": "^1.0.8",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1"
  },
  "bundlesize": [
    {
      "path": "./dist/size-animate.js",
      "maxSize": "3.4 kB"
    },
    {
      "path": "./dist/size-animate-style.js",
      "maxSize": "2.9 kB"
    },
    {
      "path": "./dist/size-timeline.js",
      "maxSize": "4.3 kB"
    },
    {
      "path": "./dist/size-spring.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./dist/size-webpack-animate.js",
      "maxSize": "3.4 kB"
    }
  ]
}
