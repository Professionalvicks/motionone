{
  "npmClient": "yarn",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "lint"
      ]
    },
    "measure": {
      "dependsOn": [
        "build"
      ]
    },
    "publish": {
      "dependsOn": [
        "build",
        "test",
        "lint"
      ]
    },
    "dev": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    }
  }
}
